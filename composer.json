{
    "name": "eoffice/riau",
    "type": "project",
    "description": "Electronic Office Province of Riau",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.3|>=8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-zip": "*",
        "barryvdh/laravel-dompdf": ">=0.9.0",
        "berkayk/onesignal-laravel": "^1.0",
        "daltcore/lara-pdf-merger": "^2.0",
        "doctrine/dbal": "^2.10",
        "dyrynda/laravel-cascade-soft-deletes": "^4.1",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^3.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.7",
        "irebega/docx-replacer": "^1.1",
        "laravel/framework": "^9.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "laravelcollective/html": "^6.1",
        "laravolt/avatar": "^4.0",
        "league/flysystem-ftp": "^3.0",
        "madnest/madzipper": "^1.2",
        "ojisatriani/fungsi": "^1.0",
        "phpoffice/phpword": "^0.18.3",
        "psr/simple-cache": "2.0",
        "setasign/fpdi": "^2.3",
        "tecnickcom/tcpdf": "^6.5",
        "yajra/laravel-datatables": "^1.0"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^1.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories",
            "Database\\Seeders\\": "database/seeders/"
        },
        "classmap": [
            "database/seeders",
            "database/factories"
        ],
        "file": [
            "app/Helpers/Help.php",
            "app/Helpers/Mapping.php",
            "app/Helpers/Gambar.php",
            "app/Helpers/Esign.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
